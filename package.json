{
  "name": "isomorphic_react_boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "scripts": {
    "clear": "rimraf ./public/*",
    "build:dev": "npm run clear && webpack -d -w --progress",
    "build:prod": "npm run clear && webpack -p --progress",
    "serve:legacy": "nodemon ./public/server.js",
    "serve": "pm2 start ./processes.json",
    "start": "npm-run-all --parallel build serve"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "isomorphic",
    "boilerplate"
  ],
  "author": "Ahmad Mirzaei (@a_m_dev)",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "moment-jalaali": "^0.8.3",
    "node-fetch": "^2.2.1",
    "pm2": "^3.5.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-spring": "^8.0.25",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
